{
  "name": "automated-hash-reports",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "npx serve -l 3001 public",
    "serve": "npx serve -l 3001 public",
    "refresh": "node scripts/refresh.js",
    "discover": "node scripts/discoveryquery.js --all --hours=24",
    "backfill": "node scripts/backfill-reports-index.js --reports-dir public/reports",
